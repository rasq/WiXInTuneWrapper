    "os": "^0.1.1",
    "path": "^0.12.7",
    "random-int": "^1.0.0",
    "remote": "^0.2.6",
    "sanitize-filename": "^1.6.1",
    "through2": "^2.0.3",
    "url": "^0.11.0",
    "uuid": "^3.2.1",
    "uuid-validate": "0.0.2",
    "xml-parse": "^0.3.1",
    "xmldom": "^0.1.27"
  }
