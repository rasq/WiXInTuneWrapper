{
  "name": "WiX-Tools-MSI-wrapper",
  "version": "1.0.0",
  "description": "A minimal Electron application",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "postinstall": "electron-builder install-app-deps",
    "build": "electron-builder -wml",
    "build-win": "electron-builder -w",
    "build-mac": "electron-builder -m",
    "build-linux": "electron-builder -l",
    "dist": "build"
  },
  "build": {
    "directories": {
      "buildResources": "App/images/build"
    },
    "linux": {
      "category": "Utility"
    }
  },
  "repository": "https://github.com/electron/electron-quick-start",
  "keywords": [
    "Electron"
  ],
  "author": "GitHub",
  "license": "CC0-1.0",
  "devDependencies": {
    "electron": "^1.7.12",
    "electron-packager": "^11.0.1"
  },
  "dependencies": {
    "async-child-process": "^1.1.1",
    "bootstrap": "^4.0.0-alpha.6",
    "child_process": "^1.0.2",
    "coffescript": "0.0.1-security",
    "dialog": "^0.3.1",
    "dialogs": "^1.1.20",
    "electron-save-file": "^1.0.2",
    "electron-settings": "^3.1.4",
    "fs": "0.0.1-security",
    "fs-extra": "^5.0.0",
    "guid": "0.0.12",
    "jquery": "^3.3.1",
    "klaw": "^2.1.1",
    "klaw-sync": "^3.0.2",
    "mkdirp": "^0.5.1",
    "mt-files-downloader": "git+https://github.com/zeltice/node-mt-files-downloader.git",
