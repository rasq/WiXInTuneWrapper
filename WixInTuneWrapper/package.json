{
  "productName": "WiX Tools MSI wrapper",
  "name": "WiX-Tools-MSI-wrapper",
  "version": "1.0.0",
  "description": "A minimal Electron based MSI builder application",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "postinstall": "electron-builder install-app-deps",
    "build": "electron-builder -wml",
    "build-win": "electron-builder -w",
    "build-mac": "electron-builder -m",
    "build-linux": "electron-builder -l",
    "dist": "build",
    "asar": "false"
  },
  "build": {
    "directories": {
      "buildResources": "App/images/build"
    },
    "linux": {
      "category": "Utility"
    }
  },
  "repository": "https://github.com/rasq/WiXInTuneWrapper",
  "keywords": [
    "Electron, wixtools, msi, intune"
  ],
  "author": "Rasq",
  "license": "CC0-1.0",
  "devDependencies": {
    "electron": "^1.8.3",
    "electron-packager": "^11.0.1"
  },
  "dependencies": {
    "adm-zip": "^0.4.7",
    "async-child-process": "^1.1.1",
    "bootstrap": "^4.0.0-alpha.6",
    "child_process": "^1.0.2",
    "coffescript": "0.0.1-security",
    "dialog": "^0.3.1",
    "dialogs": "^1.1.20",
    "electron-save-file": "^1.0.2",
    "electron-settings": "^3.1.4",
    "fs": "0.0.1-security",
    "fs-extra": "^5.0.0",
    "guid": "0.0.12",
    "jquery": "^3.3.1",
    "klaw": "^2.1.1",
    "klaw-sync": "^3.0.2",
    "mkdirp": "^0.5.1",
    "needle": "^2.2.0",
    "npm": "^5.7.1",
    "os": "^0.1.1",
    "path": "^0.12.7",
    "popper": "^1.0.1",
    "random-int": "^1.0.0",
    "remote": "^0.2.6",
    "sanitize-filename": "^1.6.1",
    "through2": "^2.0.3",
    "url": "^0.11.0",
    "uuid": "^3.2.1",
    "uuid-validate": "0.0.2",
    "winreg": "^1.2.4",
    "xml-parse": "^0.3.1",
    "xml-writer": "^1.7.0",
    "xmldom": "^0.1.27"
  }
}
